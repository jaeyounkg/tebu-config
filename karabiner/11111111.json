            ]
        },
        {
            "description": "Misc remapping for JIS symbols and stuff (apply after colemak)",
            "manipulators": [
                {   "type": "basic",
                    "from": {"key_code": "quote", "modifiers": {"optional": ["any"]}},
                    "to": [{"key_code": "semicolon"}]},
                {   "type": "basic",
                    "from": {"key_code": "international1", "modifiers": {"optional": ["any"]}},
                    "to": [{"key_code": "slash"}]},
                {   "type": "basic",
                    "from": {"key_code": "fn", "modifiers": {"optional": ["any"]}},
                    "to": [{"key_code": "grave_accent_and_tilde"}]},
                {   "type": "basic",
                    "from": {"key_code": "right_shift", "modifiers": {"optional": ["any"]}},
                    "to": [{"key_code": "fn"}]}
            ]
        },
        {
            "description": "Activate upper layer (apply after colemak)",
            "manipulators": [
                {   "type": "basic",
                    "from": {"key_code": "q", "modifiers": {"mandatory": ["right_command"], "optional": ["any"]}},
                    "to": [{"key_code": "open_bracket"}]},
                {   "type": "basic",
                    "from": {"key_code": "w", "modifiers": {"mandatory": ["right_command"], "optional": ["any"]}},
                    "to": [{"key_code": "close_bracket"}]},
                {   "type": "basic",
                    "from": {"key_code": "e", "modifiers": {"mandatory": ["right_command"], "optional": ["any"]}},
                    "to": [{"key_code": "9", "modifiers": ["left_shift"]}]},
                {   "type": "basic",
                    "from": {"key_code": "r", "modifiers": {"mandatory": ["right_command"], "optional": ["any"]}},
                    "to": [{"key_code": "0", "modifiers": ["left_shift"]}]},
                {   "type": "basic",
                    "from": {"key_code": "t", "modifiers": {"mandatory": ["right_command"], "optional": ["any"]}},
                    "to": [{"key_code": "comma", "modifiers": ["left_shift"]}]},
