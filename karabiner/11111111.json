{
    "title": "Jaeyoung Config",
    "rules": [
        {
            "description": "Spacebar as shift",
            "manipulators": [
                {   "type": "basic",
                    "from": {"key_code": "spacebar", "modifiers": {"optional": [ "any" ]}},
                    "to": [{"key_code": "left_shift"}],
                    "parameters": {"basic.to_if_alone_timeout_milliseconds": 2000},
                    "to_if_alone": [{"key_code": "spacebar"}] }
            ]
        },
        {
            "description": "Slash as shift",
            "manipulators": [
                {   "type": "basic",
                    "from": {"key_code": "slash", "modifiers": {"optional": ["any"]}},
                    "to": [{"key_code": "right_shift"}]}
            ]
        },
        {
            "description": "Apply ergonomic colemak",
            "manipulators": [
                {   "type": "basic",
                    "from": {"key_code": "b", "modifiers": {"optional": ["shift", "control", "option", "left_command"]}},
                    "to": [{"key_code": "quote"}],
                    "conditions": [{"type": "input_source_if", "input_sources": [{"language": "en"}, {"language": "ja"}]}]},
                {   "type": "basic",
                    "from": {"key_code": "semicolon", "modifiers": {"optional": ["shift", "control", "option", "left_command"]}},
                    "to": [{"key_code": "quote"}],
                    "conditions": [{"type": "input_source_if", "input_sources": [{"language": "ko"}]}]},
                {   "type": "basic",
                    "from": {"key_code": "semicolon", "modifiers": {"optional": ["shift", "control", "option", "left_command"]}},
                    "to": [{"key_code": "o"}],
                    "conditions": [{"type": "input_source_if", "input_sources": [{"language": "en"}, {"language": "ja"}]}]},

                {   "type": "basic",
                    "from": {"key_code": "w", "modifiers": {"optional": ["shift", "control", "option", "left_command"]}},
                    "to": [{"key_code": "z"}],
