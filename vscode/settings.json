{
  "explorer.confirmDelete": false,
  "editor.renderWhitespace": "boundary",
  "editor.tabCompletion": "onlySnippets",
  "editor.rulers": [80, 100],
  "window.zoomLevel": 0,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.errors.incompleteClasspath.severity": "ignore",
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  // "eslint.format.enable": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(170, 170, 170, 0.5)",
  "vim.highlightedyank.duration": 1000,
  "vim.useSystemClipboard": true,
  "vim.handleKeys": {
    "<C-w>": false,
    "<C-k>": false,
    "<C-t>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [],
  "python.linting.pylintEnabled": true,
  "python.linting.pylintArgs": ["--enable=W0614"],
  "python.formatting.autopep8Args": ["--select", "E401", "E402", "W291"],
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "*.extension": "language"
  },
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.recipes": [
    {
      "name": "latexmk ðŸ”ƒ",
      "tools": ["latexmk"]
    },
    {
      "name": "latexmk (latexmkrc)",
      "tools": ["latexmk_rconly"]
    },
    {
      "name": "latexmk (lualatex)",
      "tools": ["lualatexmk"]
    },
    {
      "name": "pdflatex âžž bibtex âžž pdflatexâ€ŠÃ—â€Š2",
      "tools": ["pdflatex", "bibtex", "pdflatex", "pdflatex"]
    },
    {
      "name": "Compile Rnw files",
      "tools": ["rnw2tex", "latexmk"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-xelatex",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "lualatexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-lualatex",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "latexmk_rconly",
      "command": "latexmk",
      "args": ["%DOC%"],
      "env": {}
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": ["%DOCFILE%"],
      "env": {}
    },
    {
      "name": "rnw2tex",
      "command": "Rscript",
      "args": [
        "-e",
        "knitr::opts_knit$set(concordance = TRUE); knitr::knit('%DOCFILE_EXT%')"
      ],
      "env": {}
    }
  ],
  "java.semanticHighlighting.enabled": true,
  "java.configuration.checkProjectSettingsExclusions": false,
  "terminal.integrated.inheritEnv": false,
  "editor.wordWrap": "on",
  "python.languageServer": "Microsoft",
  "C_Cpp.updateChannel": "Insiders",
  "cmake.configureOnOpen": true,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "java.refactor.renameFromFileExplorer": "autoApply",
  "[ocaml]": {
    "editor.defaultFormatter": "hackwaly.ocaml"
  },
  "gitlens.views.repositories.files.layout": "tree"
}
